{
  "name": "@vk/hhhelp",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "build:clean": "rimraf dist tsconfig.tsbuildinfo",
    "build:package": "docker ",
    "deps:update": "npm update",
    "dev": "tsx watch --conditions=development --clear-screen=false src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node ./dist/index.js",
    "test": "vitest"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@vk/config": "file:./packages/config",
    "@vk/di": "file:./packages/di",
    "@vk/errors": "file:./packages/errors",
    "@vk/health": "file:./packages/health",
    "@vk/logger": "file:./packages/logger",
    "@vk/utils": "file:./packages/utils",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "envalid": "^8.0.0",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "grammy": "^1.38.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "mongoose": "^9.1.1",
    "node-vault": "^0.10.9",
    "pdfjs-dist": "^5.4.530",
    "prom-client": "^15.1.3",
    "winston": "^3.19.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vk/types": "file:./packages/types",
    "rimraf": "^6.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "imports": {
    "#*": {
      "development": "./src/*.ts",
      "default": "./dist/*.js"
    },
    "#db": {
      "development": "./src/db/index.ts",
      "default": "./dist/db/index.js"
    },
    "#middleware": {
      "development": "./src/middleware/index.ts",
      "default": "./dist/middleware/index.js"
    },
    "#templates/*": {
      "development": "./src/templates/*.ts",
      "default": "./dist/templates/*.js"
    }
  }
}
