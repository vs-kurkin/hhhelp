{
  "name": "hhhelp",
  "version": "1.0.0",
  "description": "HeadHunter Helper Bot",
  "author": "vs-kurkin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vs-kurkin/hhhelp.git"
  },
  "bugs": {
    "url": "https://github.com/vs-kurkin/hhhelp/issues"
  },
  "homepage": "https://github.com/vs-kurkin/hhhelp#readme",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsup && npm run view:build",
    "build:clean": "rimraf dist public && mkdir public",
    "build:package": "docker ",
    "view:install": "cd view && pnpm install",
    "view:build": "cd view && pnpm build",
    "view:dev": "cd view && pnpm dev",
    "deps:update": "npm update",
    "dev": "tsx watch --conditions=development --clear-screen=false src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node ./dist/index.js",
    "test": "vitest run"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@vk-public/config": "^1.0.3",
    "@vk-public/di": "^1.0.3",
    "@vk-public/errors": "^1.0.3",
    "@vk-public/health": "^1.0.3",
    "@vk-public/logger": "^1.0.3",
    "@vk-public/utils": "^1.0.3",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "envalid": "^8.0.0",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "grammy": "^1.38.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "mongoose": "^9.1.1",
    "node-vault": "^0.10.9",
    "pdfjs-dist": "^5.4.530",
    "prom-client": "^15.1.3",
    "winston": "^3.19.0",
    "winston-transport": "^4.9.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vk-public/types": "^1.0.3",
    "rimraf": "^6.0.0",
    "tsup": "^8.5.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.17"
  },
  "imports": {
    "#*": {
      "development": "./src/*.ts",
      "default": "./dist/*.js"
    },
    "#db": {
      "development": "./src/db/index.ts",
      "default": "./dist/db/index.js"
    },
    "#middleware": {
      "development": "./src/middleware/index.ts",
      "default": "./dist/middleware/index.js"
    },
    "#templates/*": {
      "development": "./src/templates/*.ts",
      "default": "./dist/templates/*.js"
    }
  }
}
